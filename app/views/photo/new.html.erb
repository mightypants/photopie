<h1>Photo#new</h1>
<p>Find me in app/views/photo/new.html.erb</p>

<div class="photos new">
  <h2>Create Photo</h2>

  <%= form_for(:photo, :url => {:action => "create"}, :html => { :multipart => true }) do |f| %>

    <%= render(:partial => "form", :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= submit_tag("Create Photo") %>
    </div>

  <% end %>
</div>

